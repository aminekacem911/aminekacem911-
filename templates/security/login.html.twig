<html>

<head>
        <meta charset="UTF-8">

        {% block stylesheets %}

        <link href="/admin_files/css/font-face.css" rel="stylesheet" media="all">
        <link href="/admin_files/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
        <link href="/admin_files/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
        <link href="/admin_files/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

        <link href="/admin_files/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

        <link href="/admin_files/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
        <link href="/admin_files/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet"
                media="all">
        <link href="/admin_files/vendor/wow/animate.css" rel="stylesheet" media="all">
        <link href="/admin_files/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
        <link href="/admin_files/vendor/slick/slick.css" rel="stylesheet" media="all">
        <link href="/admin_files/vendor/select2/select2.min.css" rel="stylesheet" media="all">
        <link href="/admin_files/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">


        <link href="/admin_files/css/theme.css" rel="stylesheet" media="all">
        {% endblock %}
</head>
<div class="animsition">
        <div class="page-wrapper">
                <div class="page-content--bge5">
                        <div class="container">
                                <div class="login-wrap">
                                        <div class="login-content">
                                                <div class="login-logo">
                                                        {% for message in app.flashes('error') %}
                                                        <div class="alert alert-error">
                                                                {{ message }}
                                                        </div>
                                                        {% endfor %}
                                                </div>
                                                <div class="login-form">
                                                        <form action="" method="post">
                                                                <input type="hidden" name="_csrf_token"
                                                                        value="{{ csrf_token('authenticate') }}">

                                                                <div class="form-group">
                                                                        <label>Email Address</label>
                                                                        <input class="au-input au-input--full"
                                                                                type="email" name="email"
                                                                                placeholder="Email">
                                                                </div>
                                                                <div class="form-group">
                                                                        <label>Password</label>
                                                                        <input class="au-input au-input--full"
                                                                                type="password" name="password"
                                                                                placeholder="Password">
                                                                </div>
                                                                <div class="login-checkbox">
                                                                        <label>
                                                                                <input type="checkbox"
                                                                                        name="remember">Remember Me
                                                                        </label>

                                                                </div>
                                                                <button class="au-btn au-btn--block au-btn--green m-b-20"
                                                                        type="submit">sign in</button>
                                                                <div class="social-login-content">

                                                                </div>
                                                        </form>

                                                </div>
                                        </div>
                                </div>
                        </div>
                </div>

        </div>

</div>

</body>

